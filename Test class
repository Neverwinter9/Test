class Car {
let wheels: Int = 4
var color: String
var engVol: Double

init(wheels: Int, color: String, engVol: Double){
self.wheels = wheels
self.color = color
self.engVol = engVol
}

func ride(action: String){
  print(The car \(action))
 }
}

var bmw = Car(wheels: 4, color: "Black", engVol: 3.0)
bmw.ride(action: "rides")
